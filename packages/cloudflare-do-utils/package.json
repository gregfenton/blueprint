{
  "name": "@transformation-dev/cloudflare-do-utils",
  "version": "0.7.0",
  "description": "Utilities for working with Cloudflare's Durable Objects",
  "keywords": [
    "cloudflare",
    "durable object",
    "durable objects",
    "do",
    "DurableObject",
    "DurableObjects"
  ],
  "author": "Larry Maccherone <Larry@Maccherone.com>",
  "homepage": "https://github.com/transformation-dev/blueprint/tree/master/packages/cloudflare-do-utils#readme",
  "license": "MIT",
  "main": "cloudflare-do-utils.js",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/transformation-dev/blueprint.git#master"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "npx vite build",
    "test": "npm run build && npm run test:run",
    "test:run": "NODE_OPTIONS=--experimental-vm-modules vitest --run",
    "test:watch": "npx vite && NODE_OPTIONS=--experimental-vm-modules vitest",
    "coverage": "npx vite build && NODE_OPTIONS=--experimental-vm-modules vitest run --coverage"
  },
  "bugs": {
    "url": "https://github.com/transformation-dev/blueprint/issues"
  },
  "dependencies": {
    "@cfworker/json-schema": "^1.12.5",
    "cbor-x": "^1.5.2",
    "debug": "^4.3.4",
    "deep-object-diff": "^1.1.9",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@vitest/coverage-c8": "^0.29.8",
    "vitest": "^0.30.1",
    "vitest-environment-miniflare": "^2.13.0"
  }
}
